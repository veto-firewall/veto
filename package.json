{
  "name": "veto",
  "version": "2.1.5",
  "description": "Advanced filtering with GeoIP, ASN, domain, URL and content blocking capabilities. Simple Firewall for Privacy and Security",
  "main": "index.js",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack --mode production --config webpack.config.cjs",
    "package": "npm run build && cd dist && zip -r veto.zip * && cd ..",
    "package:win": "npm run build && cd dist && powershell Compress-Archive -Path * -DestinationPath veto.zip -Force && cd ..",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,html,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,html,css}\"",
    "check": "npm run lint && npm run format:check",
    "fix": "npm run lint:fix && npm run format",
    "web-ext:lint": "npm run build && cd dist && web-ext lint"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "countries-list": "^3.1.1",
    "ipaddr.js": "^2.2.0",
    "lru-cache": "^11.1.0",
    "mmdb-lib": "^3.0.0",
    "tar-stream": "^3.1.7",
    "validator": "^13.15.0"
  },
  "devDependencies": {
    "@types/firefox-webext-browser": "^120.0.4",
    "@types/tar-stream": "^3.1.3",
    "@types/validator": "^13.15.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.8",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1"
  }
}
